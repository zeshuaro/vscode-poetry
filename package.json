{
  "name": "vscode-python-poetry",
  "displayName": "Python Poetry",
  "description": "Python Poetry support for VS Code to manage Poetry commands",
  "version": "0.0.1",
  "publisher": "zeshuaro",
  "repository": {
    "type": "git",
    "url": "https://github.com/zeshuaro/vscode-poetry"
  },
  "engines": {
    "vscode": "^1.66.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:vscode-python-poetry.addPackage",
    "onCommand:vscode-python-poetry.addDevPackage",
    "onCommand:vscode-python-poetry.removePackage",
    "onCommand:vscode-python-poetry.removeDevPackage"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "vscode-python-poetry.addPackage",
        "title": "Add package",
        "category": "Poetry"
      },
      {
        "command": "vscode-python-poetry.addDevPackage",
        "title": "Add dev package",
        "category": "Poetry"
      },
      {
        "command": "vscode-python-poetry.removePackage",
        "title": "Remove package",
        "category": "Poetry"
      },
      {
        "command": "vscode-python-poetry.removeDevPackage",
        "title": "Remove dev package",
        "category": "Poetry"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "yarn run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "yarn run compile && yarn run lint",
    "lint": "eslint src --ext ts && prettier src/**/*.ts --check",
    "format": "eslint src --ext ts --fix && prettier src/**/*.ts --write",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/glob": "^7.2.0",
    "@types/mocha": "^9.1.0",
    "@types/node": "14.x",
    "@types/sinon": "^10.0.11",
    "@types/vscode": "^1.66.0",
    "@typescript-eslint/eslint-plugin": "^5.16.0",
    "@typescript-eslint/parser": "^5.16.0",
    "@vscode/test-electron": "^2.1.3",
    "eslint": "^8.11.0",
    "glob": "^7.2.0",
    "mocha": "^9.2.2",
    "nyc": "^15.1.0",
    "prettier": "^2.6.2",
    "sinon": "^13.0.2",
    "typescript": "^4.5.5"
  }
}
